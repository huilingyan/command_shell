#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int seq2(int x) {
  return 4 * x - 3;
}

void sumSeq2(int low, int high) {
  if (low >= high) {
    printf("\n");
  }
  else {
    int i = low;
    while (i < high - 1) {
      printf("%d, ", seq1(i));
      i++;
    }
    printf("%d\n", seq1(high - 1));
  }
}

void test_seq2(int x) {
  int y = seq2(x);
  printf("seq2(%d) = %d\n", x, y);
}

void test_sumSeq2(int low, int high) {
  printf("printSeq1Range(%d, %d)\n", low, high);
  sumSeq2(low, high);
}

int main() {
  test_seq2(0);
  test_seq2(1);
  test_seq2(-1);
  test_seq2(-4);
  test_seq2(10);
  test_seq2(-32768);
  test_seq2(32767);

  test_sumSeq2(0, 0);
  test_sumSeq2(1, 1);
  test_sumSeq2(0, 4);
  test_sumSeq2(-2, 6);
  test_sumSeq2(7, 2);
  test_sumSeq2(-6, -2);
  test_sumSeq2(-6, -7);
}
